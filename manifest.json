{
"name": "Productify",
"version": "1.0",
"manifest_version": 2,
"permissions": [
  "tabs",
  "storage",
  "alarms"
],

"background": {
  "scripts": ["background.js"]
},

"browser_action": {
  "default_title": "Peaceful productivity everywhere you surf",
  "default_popup": "hover.html"
},

"description": "Peaceful productivity everywhere you surf.",

"icons": {
  "16": "images/brain.png",
  "48": "images/brain.png",
  "128": "images/brain.png"
}
}
