{
	"name": "Prductify",
	"version":"1.0",
	"manifest_version": 2,
	"permissions": [
		"tabs",
		"storage",
		"alarms"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["content.js"]
		}
	],
	"background":{
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_title": "Productify"
	}
}
